    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Awful.Mobile.Views.ProbationView">
    <ContentView.Content>
        <StackLayout BackgroundColor="Pink">
            <Label TextColor="Black" HorizontalTextAlignment="Center" FontSize="10" Text="{Binding OnProbationText}"/>
            <Rectangle BackgroundColor="Black"/>
        </StackLayout>
    </ContentView.Content>
</ContentView>
